<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <size
                android:width="15dp"
                android:height="10dp" />
            <solid android:color="@android:color/black" />
        </shape>
    </item>

    <item
        android:left="-7dp"
        android:right="12dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white" />
            </shape>
        </rotate>
    </item>

    <item
        android:left="-10dp"
        android:right="15dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/black" />
            </shape>
        </rotate>
    </item>

    <item
        android:bottom="2dp"
        android:left="4dp"
        android:right="-20dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white"></solid>
            </shape>
        </rotate>
    </item>

    <item
        android:left="4dp"
        android:right="-20dp"
        android:top="2dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white"></solid>
            </shape>
        </rotate>
    </item>
</layer-list>